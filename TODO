#[doc = "Maps a primitive value to the IRI for its type."]
fn primitive_to_iri(value: primitive) -> iri
{
	alt value
	{
		xml_P(_)		{'http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral'}
		string_P(_)	{'http://www.w3.org/2001/XMLSchema#string'}	// or xsd:string
		// TODO: add the rest
	}
}

